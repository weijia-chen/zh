<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.weijia.zh.user.dao.FollowTopicDao">

    <select id="searchUserFollowTopics" resultType="_long">
        SELECT topic_id
        FROM tb_follow_topic
        where user_id = #{userId}
    </select>


</mapper>